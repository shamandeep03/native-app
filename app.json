{
  "expo": {
    "name": "app-native",
    "slug": "app-native",
    "version": "1.0.0",
     "jsEngine": "hermes",
     
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
    "plugins": ["expo-dev-launcher","expo-location"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shaman123.appnative"
    },
    "android": {
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shaman123.appnative"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
       "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eb182883-829e-4fd0-8ade-a97defcd7e00"
      }
    }
  }
}
